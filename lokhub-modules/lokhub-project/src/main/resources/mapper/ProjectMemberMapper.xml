<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.laigeoffer.lokhub.project.mapper.ProjectMemberMapper">
    <select id="searchMember" parameterType="java.lang.String" resultType="com.laigeoffer.lokhub.project.domain.vo.project.member.ProjectMemberResVO">
        select pm.user_id, pm.joined_time from lokhub_project_member pm
        where pm.pt_id = #{data.projectId}
    </select>

    <select id="queryExecutorList" parameterType="java.lang.String" resultType="com.laigeoffer.lokhub.project.domain.vo.project.member.ProjectMemberResVO">
        select pm.user_id from lokhub_project_member pm
        where pm.pt_id = #{projectId}
    </select>
    <select id="queryTaskUserList" parameterType="java.lang.String" resultType="com.laigeoffer.lokhub.project.domain.vo.project.member.ProjectMemberResVO">
        select pm.user_id from lokhub_project_member pm
        where pm.pt_id = #{taskId}
    </select>
</mapper>